# mATRIC

row = int(input("Enter rows: "))
colum = int(input("Entr colum: "))
matrix = []
print("enter row-wise-colum: ")
for i in range(row):
    temp_row = []
    for j in range(colum):
        temp_row.append(int(input()))
    matrix.append(temp_row)

print("\n 2D matrix is: ")
for i in range(row):
    for j in range(colum):
        print(matrix[i][j], end= " ")
    print()
            
